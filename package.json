{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cp .env.development .env.local && next dev",
    "stage": "cp .env.staging .env.local && next dev",
    "prod": "cp .env.production .env.local && next dev",
    "turbo": "next dev --turbopack",
    "build:dev": "cp .env.development .env.local && next build",
