    "start:dev": "cp .env.development .env.local && next start",
    "start:stage": "cp .env.staging .env.local && next start",
    "start:prod": "cp .env.production .env.local && next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.7.0",
    "formik": "^2.4.6",
